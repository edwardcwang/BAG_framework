savedComponent = (let
(tmp1 tmp2)
(unless tmp1
tmp1 = make_sevVariableStruct()
)
(unless tmp1
tmp1 = (ncons nil)
)
tmp1->name = "tstep"
tmp1->expression = "1p"
tmp1->index = 1
(unless tmp2
tmp2 = make_sevVariableStruct()
)
(unless tmp2
tmp2 = (ncons nil)
)
tmp2->name = "tsim"
tmp2->expression = "5n"
tmp2->index = 2
list(tmp1 tmp2)
)
